  <data name="PageSize.Help" xml:space="preserve">
    <value>Bij een ongeldig getal of een getal kleiner of gelijk aan 0 wordt een default waarde van 25 gehanteerd</value>
  </data>
  <data name="PageSizeLabel.Text" xml:space="preserve">
