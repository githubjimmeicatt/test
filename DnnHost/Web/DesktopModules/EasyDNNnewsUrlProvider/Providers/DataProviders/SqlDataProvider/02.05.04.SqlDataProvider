IF NOT EXISTS (SELECT name FROM sys.indexes WHERE name = N'IX_{objectQualifier}EasyDNNnewsUrlNewProviderData_TabID')
	CREATE NONCLUSTERED INDEX [IX_{objectQualifier}EasyDNNnewsUrlNewProviderData_TabID] ON {databaseOwner}[{objectQualifier}EasyDNNnewsUrlNewProviderData] ([TabID] ASC)  INCLUDE(LinkTitle)
GO
